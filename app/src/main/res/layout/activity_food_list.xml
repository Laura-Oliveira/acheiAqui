<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.acheiAqui.Control.RegisterFoodListActivity"
    android:orientation="vertical">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/registered_shop"
        android:text="@string/food_list"
        android:textSize="14pt"
        android:textColor="@color/btn_text_color"
        android:background="@color/btn_color"
        android:textAlignment="center"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.google.android.material.checkbox.MaterialCheckBox
            android:id="@+id/check_box_comida"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/check_comida"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/check_comida"
            android:textColor="@color/colorPrimary"
            android:textSize="10pt"
            android:textStyle="bold"
            android:layout_marginBottom="8pt"
            />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.google.android.material.checkbox.MaterialCheckBox
            android:id="@+id/check_box_comida1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/check_comida1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/check_comida1"
            android:textColor="@color/colorPrimary"
            android:textSize="10pt"
            android:textStyle="bold"
            android:layout_marginBottom="8pt"
            />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.google.android.material.checkbox.MaterialCheckBox
            android:id="@+id/check_box_comida2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/check_comida2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/check_comida2"
            android:textColor="@color/colorPrimary"
            android:textSize="10pt"
            android:textStyle="bold"
            android:layout_marginBottom="8pt"
            />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.google.android.material.checkbox.MaterialCheckBox
            android:id="@+id/check_box_comida3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/check_comida3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/check_comida3"
            android:textColor="@color/colorPrimary"
            android:textSize="10pt"
            android:textStyle="bold"
            android:layout_marginBottom="8pt"
            />
    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.google.android.material.checkbox.MaterialCheckBox
            android:id="@+id/check_box_comida4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/check_comida4"
